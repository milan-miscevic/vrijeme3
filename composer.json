{
    "name": "milan-miscevic/vrijeme3",
    "type": "project",
    "description": "Planned collector of weather data",
    "keywords": [
        "inert",
        "weather data",
        "example project",
        "weather",
        "skeleton application"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Milan Miščević",
            "email": "milan.miscevic@gmail.com"
        }
    ],
    "require": {
        "php": "^8",
        "milan-miscevic/inert": "^0.13"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "phpstan/phpstan": "^0.12.96",
        "phpunit/phpunit": "^9.5.6",
        "vimeo/psalm": "^4.8.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vrijeme3\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vrijeme3\\Tests\\": "tests/"
        }
    }
}
